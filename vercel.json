{
  "version": 2,
  "public": true,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_FORCE_REBUILD": "true"
    }
  },
  "redirects": [
    {
      "source": "http://printboothpro.com",
      "destination": "https://www.printboothpro.com",
      "permanent": true
    },
    {
      "source": "https://printboothpro.com",
      "destination": "https://www.printboothpro.com",
      "permanent": true
    },
    {
      "source": "http://printboothpro-b3coew2m5-francoise-tonetos-projects.vercel.app",
      "destination": "https://www.printboothpro.com",
      "permanent": true
    },
    {
      "source": "https://printboothpro-b3coew2m5-francoise-tonetos-projects.vercel.app",
      "destination": "https://www.printboothpro.com",
      "permanent": true
    }
  ]
} 